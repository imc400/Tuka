{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "development-device": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "production",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "production",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "testflight": {
      "distribution": "store",
      "environment": "production",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "autoIncrement": true
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "testflight": {
      "ios": {
        "appleId": "igblancora@gmail.com",
        "ascAppId": "6755695544",
        "appleTeamId": "Y763D7J83C"
      }
    },
    "production": {}
  }
}
